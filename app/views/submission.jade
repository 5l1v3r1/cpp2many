extends layout

block content

    .jumbotron(style='padding: 1em;')= title


    .panel.panel-default
        .panel-heading Output
            .pull-right(style='position: relative; margin-top: -7px; margin-bottom: -20px;')
                button#copy-clipboard-btn.btn.btn-default(data-clipboard-target='#output', title='Copy to clipboard')
                    i.fa.fa-clipboard
        .panel-body
            pre(style='background: white; border: 0px; padding: 0px;')
                code#output= out

    if err != ""
        .panel.panel-default
            .panel-heading Errors
            .panel-body
                pre(style='background: white; border: 0px; padding: 0px;')
                    code= err

    .panel.panel-default
        .panel-heading Input
        .panel-body
            pre(style='background: white; border: 0px; padding: 0px;')
                code= inp

    if 0
        .panel.panel-default
            .panel-heading Meta
            .panel-body
                pre(style='background: white; border: 0px; padding: 0px;')
                    code= meta

    script.
        start_submission()
